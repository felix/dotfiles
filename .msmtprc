# Set default values for all following accounts.
defaults
port            587
protocol        smtp
auth            on
tls             on
tls_starttls    on
tls_certcheck   on
#tls_trust_file  /usr/local/share/certs/ca-root-nss.crt
syslog          LOG_MAIL

account         userspace
host            mail.userspace.com.au
from            felix@userspace.com.au
user            felix@userspace.com.au
#passwordeval    "echo $(gpg2 --no-tty -qd ~/Documents/passwords.gpg | sed -n 's,^machine mail.userspace.com.au .*password \([^ ]*\).*,\1,p')"
passwordeval    "pass show userspace/mail.userspace.com.au"
tls_fingerprint 00:A5:6F:7A:C3:71:70:32:21:FD:BB:13:80:AE:47:49:C0:0E:7B:5F:EF:56:A3:C2:E1:EF:20:73:BF:32:95:AE

account default : userspace
