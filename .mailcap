application/msword; mutt_bgrun soffice '%s'
application/vnd.ms-excel; mutt_bgrun soffice '%s'
application/vnd.openxmlformats-officedocument.wordprocessingml.document; mutt_bgrun soffice '%s'

# mutt
application/msword; antiword %s ; copiousoutput
text/html         ; elinks -dump 1 -dump-charset iso-8859-15 -default-mime-type text/html %s ; needsterminal ; copiousoutput
text/htm          ; elinks -dump 1 -dump-charset iso-8859-15 -default-mime-type text/html %s ; needsterminal ; copiousoutput
message/html      ; elinks -dump 1 -dump-charset iso-8859-15 -default-mime-type text/html %s ; needsterminal ; copiousoutput
message/htm       ; elinks -dump 1 -dump-charset iso-8859-15 -default-mime-type text/html %s ; needsterminal ; copiousoutput
image/svg+xml     ; inkscape %s ;
image/*           ; mutt_bgrun feh --fullscreen --auto-zoom %s ; copiousoutput

image/*           ; anytopnm %s | pnmscale -xsize 80 -ysize 50 | ppmtopgm | pgmtopbm | pbmtoascii ; copiousoutput
application/x-gunzip  ; gzcat; copiousoutput
application/x-tar-gz  ; gunzip -c %s | tar -tf - ; copiousoutput
application/postscript; ps2ascii %s; copiousoutput
application/pdf; zathura '%s'
